            <kind>1</kind>
            <string>switch Estado {

    case "inicio":
        if vida &lt; 15 {
            vida += 0.2
            audio_play_sound(sfx_carregar, 1, false)
        }
        else {
            audio_stop_sound(sfx_carregar)
            vida = 15
            switch room {
            
                case room0:
                    audio_play_sound(mus_game, 10, true)
                break;
                
                case room4:
                    audio_play_sound(mus_yellow, 10, true)
                break;
            
            }
            Estado = "parado"
        }
    break;

    case "parado":
    if mouse_check_button_pressed(mb_left) {
        scr_mov()
    }
    
    event_user(0)
    
    if keyboard_check_direct(ord('Z')) and alarm[2] == -1 {
        for (i = 0; i &lt; balas; i += 1) {
            bola = instance_create(x + 32, y + 48, obj_bola)
            bola.direction = (360/balas)*i
        }
        alarm[2] = 10
        bolas = true
    }
    if keyboard_check_released(ord('Z')) {
        bolas = false
    }
    break;
    
    case "primeiro":
    break;
    
    case "movendo":
    background_x = -(x - 64)/8
    background_y = -(y - 64)/8
    bolas = false
    if point_distance(x,y,x1,y1) &gt; 8 {
        move_towards_point(x1,y1, sp)
    }
    else {
        x = x1
        y = y1
        speed = 0
        Estado = "parado"
    }
    break;
    
    case "morto":
        if alp &lt; 1 {
            alarm[7] = 130
        }  
    break;    

}

if gameover {
    if keyboard_check_pressed(ord('R')) {room_restart()}
}


</string>

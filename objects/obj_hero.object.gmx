            <string>switch Estado {

    case "inicio":
        if obj_boss.vida &gt;= 15 {
            fast = -sp
            Estado = "movendo"
        }
    break

    case "movendo":
    if collision_rectangle(0,0,room_width, room_height/2, obj_boss, false, false) {
        x += fast
        if point_distance(x,443,660,443) &lt; 5{
            Estado = "subindo"
        }
        if point_distance(x,443,60,443) &lt; 5{
            Estado = "subindo"
        }  
    }
    if collision_rectangle(0,room_height/2,room_width, room_height, obj_boss, false, false) {
        x += fast
        if point_distance(x,443,660,443) &lt; 5{
            fast = -sp
        }
        if point_distance(x,443,60,443) &lt; 5{
            fast = sp
        }
        
        if alarm[4] == -1 {
            count = 0
            alarm[2] = 1
            fast = 0
            bala = 3
            Estado = "atirando"
        }
        if collision_circle(x,y,40,obj_bola,true,true) {
            if x &lt; room_width/2 {
                dire = 1
            }
            else {
                dire = -1
            }
            dura = 0
            audio_play_sound(sfx_dash, 0, false)
            Estado = "dash"
        }   
    }
    break;
    
    case "dash":
        if dura &lt; 30 {
            dura += 2
            x += dire*dura
        }
        else {
            fast = choose(sp, -sp)
            Estado = "movendo"
        }
        scr_trace()
    break;
    
    case "subindo":
        y -= sp
        if point_distance(660,y,660,150) &lt; 7{
            y = 151
            if alarm[4] == -1 {
                count = 0
                alarm[2] = 1
                fast = 0
                bala = 100
                Estado = "atirando"
            } 
        }
        if point_distance(60,y,60,150) &lt; 7{
            y = 151
            if alarm[4] == -1 {
                count = 0
                alarm[2] = 1
                fast = 0
                bala = 100
                Estado = "atirando"
            } 
        }
        if collision_rectangle(0,room_height/2,room_width, room_height, obj_boss, false, false) {
            pico = 0
            Estado = "pulando"
        }
    break;
    
    case "pulando":
    if x &lt; room_width/2 {
        dire = 1
    }
    else {
        dire = -1
    }
    if pico &lt; 20 {
        x += dire*5
        y -= 5
        pico += 5
    }
    else {
        gralha = 0
        Estado = "caindo"
    }
    break;
    
    case "caindo":
        if y &lt; 443 {
            gralha += 0.45
            x += dire*3
            y += gralha
        }
        else {
            y = 443
            fast = choose(sp, -sp)
            Estado = "movendo"
        }
    break;
    
    case "dano":
    if status == "movendo" {
        x += fast
    }
    break;
    
    case "atirando":
    break;
    
    case "morto":
    if y &lt; 443 {
        gralha += 0.45
        y += gralha
    }
    else {
        y = 443
    }
    break;    

}

